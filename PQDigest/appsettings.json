{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": "Error",
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Data Source=localhost; Initial Catalog=PQDigest; Integrated Security=SSPI",
          "tableName": "Log"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
